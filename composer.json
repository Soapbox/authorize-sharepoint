{
    "name": "soapbox/authorize-sharepoint",
    "description": "SharePoint authorization strategy for SoapBox/Authorize.",
    "keywords": ["authentication", "sharepoint"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jaspaul/sharepoint-oauth-app-client",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/jaspaul/sharepoint-oauth-app-client.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "php": ">=5.4.0",
                    "guzzlehttp/guzzle": "4.1.*",
                    "firebase/php-jwt": "1.*",
                    "nesbot/carbon": "~1.0"
                },
                "require-dev": {
                    "phpunit/phpunit": "~4.3"
                },
                "autoload": {
                    "psr-4": {
                        "WeAreArchitect\\SharePoint\\": "src/"
                    }
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@github.com:soapbox/authorize.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "soapbox/authorize": "dev-master",
        "illuminate/support": "4.2.*",
        "jaspaul/sharepoint-oauth-app-client": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "SoapBox\\AuthorizeSharepoint\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
