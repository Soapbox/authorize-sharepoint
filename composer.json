{
    "name": "soapbox/authorize-sharepoint",
    "description": "SharePoint authorization strategy for SoapBox/Authorize.",
    "keywords": ["authentication", "sharepoint"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "soapbox/sharepoint-client",
                "version": "dev-master",
                "source": {
                    "url": "git://github.com/soapbox/sharepoint-client.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "php": ">=5.4.0",
                    "guzzlehttp/guzzle": "~4.0",
                    "nixilla/php-jwt": "~0.1",
                    "nesbot/Carbon": "~1.9"
                },
                "autoload": {
                    "psr-4" : {
                        "SoapBox\\SharePoint\\": "src/"
                    }
                }
            }
        },
        {
            "type": "vcs",
            "url":  "git@github.com:soapbox/authorize.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "soapbox/authorize": "dev-master",
        "illuminate/support": "4.2.*",
        "soapbox/sharepoint-client": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "SoapBox\\AuthorizeSharepoint\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
